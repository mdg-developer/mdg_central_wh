        <field name="arch" type="xml">
            <xpath expr="//div" position="inside">
               <button string="Internal Transfer" name="int_transfer" type="object" class="button_int_transfer oe_highlight" id="button_click">Internal Transfer</button>
            </xpath>
        </field>
    </record>

